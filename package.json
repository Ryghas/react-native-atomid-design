{
  "name": "shopping-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "start-clear": "yarn start --clear",
    "start:staging": "NODE_ENV=staging yarn start",
    "start-clear:staging": "yarn start:staging --clear",
    "android": "expo run:android",
    "android:staging": "NODE_ENV=staging yarn android",
    "ios": "expo run:ios",
    "ios:staging": "NODE_ENV=staging yarn ios",
    "prebuild": "expo prebuild",
    "prebuild-clean": "yarn prebuild --clean",
    "prebuild:staging": "NODE_ENV=staging yarn prebuild",
    "prebuild-clean:staging": "yarn prebuild:staging --clean",
    "build:sync": "eas build:version:sync",
    "build:android": "eas build --platform android --clear-cache --build-logger-level debug",
    "build:android:staging": "NODE_ENV=staging ./scripts/load-env.sh .env.staging yarn build:sync --platform android && ./scripts/load-env.sh .env.staging yarn build:android --profile staging",
    "build:ios": "eas build --platform ios --clear-cache --build-logger-level debug",
    "build:ios:staging": "NODE_ENV=staging ./scripts/load-env.sh .env.staging yarn build:sync --platform ios && ./scripts/load-env.sh .env.staging yarn build:ios --profile staging",
    "update:staging": "NODE_ENV=staging ./scripts/load-env.sh .env.staging eas update --branch staging --clear-cache",
    "eas-build-pre-install": "./scripts/eas-build-pre-install.sh",
    "eas-build-post-install": "./scripts/eas-build-post-install.sh",
    "test": "jest -o"
  },
  "dependencies": {
    "@amplitude/analytics-react-native": "^1.4.9",
    "@bacons/link-assets": "^1.1.0",
    "@gorhom/bottom-sheet": "^5.2.7",
    "@notifee/react-native": "^7.8.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.5.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-firebase/analytics": "^21.0.0",
    "@react-native-firebase/app": "^21.0.0",
    "@react-native-firebase/crashlytics": "^21.0.0",
    "@react-native-firebase/dynamic-links": "^21.0.0",
    "@react-native-firebase/messaging": "^21.0.0",
    "@react-native-firebase/perf": "^21.0.0",
    "@react-native-firebase/remote-config": "^21.0.0",
    "@react-navigation/native": "^7.1.24",
    "@react-navigation/native-stack": "^7.8.5",
    "@shopify/flash-list": "2.2.0",
    "@storybook/react-native": "^10.1.0",
    "expo": "54.0.20",
    "expo-application": "~7.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-cellular": "~8.0.7",
    "expo-chucker": "^0.1.5",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
